{
	"name": "Metadata Stripper",
	"version": "0.1",
	"description": "Removes unneeded metadata from URLs",
	"background": { 
		"scripts": ["background.js"]
	},
	"icons": {
		//"32": "icon32.png"
	},
	"manifest_version": 2,
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		
		"*://www.facebook.com/*",
		"*://www.imdb.com/*",
		"*://www.youtube.com/*",
		"*://twitter.com/*"
	]
}
